{
  "name": "online_design_tool",
  "version": "1.0.0",
  "private": true,
  "description": "A network design and learning tool web application for teaching networking concepts.",
  "author": "Llewellyn Brunsdon-Haland",
  "scripts": {
    "vue-dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "vue-ui": "vue ui",
    "dev": "nodemon app.js",
    "dev-debug": "nodemon --watch app.js --inspect app.js",
    "start": "npm run migrate && node app",
    "prisma-studio": "npx prisma studio",
    "makemigrations": "npx prisma migrate dev",
    "migrate": "npx prisma migrate deploy",
    "test": "mocha -r esm",
    "deploy-production": "git checkout production && git merge master -m 'Merged master into production' && git push && git checkout master"
  },
  "main": "app.js",
  "dependencies": {
    "@prisma/client": "^3.5.0",
    "@vue/cli": "^4.5.14",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.6.0",
    "bootstrap-icons": "^1.4.0",
    "bootstrap-vue": "^2.21.2",
    "browserify": "^16.5.2",
    "connect-pg-simple": "^7.0.0",
    "core-js": "^3.6.5",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.7",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "implement-js": "0.0.27",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20",
    "p5": "^1.2.0",
    "passport": "^0.5.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "popper.js": "^1.16.1",
    "portal-vue": "^2.1.7",
    "prisma": "^3.5.0",
    "rfdc": "^1.2.0",
    "serve-index": "^1.9.1",
    "socket.io": "^2.4.1",
    "vue": "^2.6.14",
    "vue-p5": "^0.8.4",
    "vue-router": "^3.4.9",
    "vue-template-compiler": "^2.6.14"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.8",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "^4.5.9",
    "@vue/cli-plugin-unit-mocha": "~4.5.0",
    "@vue/cli-service": "^4.5.13",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "esm": "^3.2.25",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.14",
    "puppeteer": "^9.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
